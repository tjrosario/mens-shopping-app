  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:staging": "run-s build:client-staging build:aot build:server-staging",
    "build:prod": "run-s build:client-prod build:aot build:server-prod",
    "build:client-staging": "ng build -prod --env=staging --build-optimizer --app 0",
    "build:client-prod": "ng build -prod --env=prod --build-optimizer --app 0",
    "build:aot": "ng build --aot --app 1",
    "build:server-staging": "webpack --config webpack.staging.config.js -p",
    "build:server-prod": "webpack --config webpack.prod.config.js -p",
    "server:start": "./scripts/start.sh",
    "server:restart": "./scripts/restart.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
